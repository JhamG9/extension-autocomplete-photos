{
  "manifest_version": 3,
  "name": "Image Jham GPT",
  "version": "1.0",
  "description": "Simular clics en elementos",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icono16.png",
      "48": "icono48.png",
      "128": "icono128.png"
    }
  },
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
